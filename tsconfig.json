{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "outDir": "./dist",      

    "jsx": "react-jsx",
    "strict": true, 
    "noImplicitAny": true,        
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true, 
    "esModuleInterop": true,                      
    "skipLibCheck": true,   
    "resolveJsonModule": true, 
    "forceConsistentCasingInFileNames": true
  }
}
